  "main": "src/pagination.less",
  "dependencies": {
    "fj-buttons": "git://fake.ghe.domain/flapjack/fj-buttons.git",
    "fj-colors": "git://fake.ghe.domain/flapjack/fj-colors.git",
    "fj-forms": "git://fake.ghe.domain/flapjack/fj-forms.git",
    "cf-typography": "git://fake.ghe.domain/flapjack/cf-typography.git",
